load "qf11to25"

int nx=2;
mesh Th=square(nx,nx);

real[int,int] qq2=[ // w_i, x_baric_i, y_baric_i
		   [1./3,  0, 0],
		   [1./3,  1, 0],
		   [1./3,  0, 1]];
QF2 qf2(1,qq2);

{
  real I1 = int2d(Th)(x);
  real I2 = int2d(Th, qft=qf2)(x);
  cout << "I1(x)=" << I1 << endl;
  cout << "I2(x)=" << I2 << endl << endl;
}
{
  real I1 = int2d(Th)(x^2);
  real I2 = int2d(Th, qft=qf2)(x^2);
  cout << "I1(x^2)=" << I1 << endl;
  cout << "I2(x^2)=" << I2 << endl << endl;
}
